    "eslint-config-prettier": "^3.3.0",
    "flow-bin": "^0.90.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  }
}
